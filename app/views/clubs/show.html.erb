<!-- Slide #73 -->
<section class="slide fade kenBurns">
  <div class="content">
    <div class="container">
      <div class="wrap">

        <div class="fix-12-12">
          <ul class="grid">
            <li class="col-6-12 left box-73">

             <%= link_to 'Edit Club', edit_club_path(@club) if @current_club%>

              <h1 class="small ae-1"><%= @club.club_name %></h1>
              <!-- <h2 class="small ae-2">Choose your Table below</h2> -->
              <!-- <div class="ae-3"><p>With EasyNight booking your table at your favorite NightClub has never been this easy.</p></div> -->
              <div class="ae-4"><p> Choose your desired table</p></div>

            </li>
            <li class="col-6-12 left ae-1 fromAbove" data-action="zoom">
              <%= image_tag @club.image_url, alt: "Club Layout Image", class: "shiftImage shiftImageVertical image-73" %>
            </li><br>
          </ul>
          <p><br> </p>
          <p><br> </p>

          <ul class="grid fixedSpaces equal center grid-73">
            <%  if not @club.tables.nil? %>
              <% @club.tables.each do |table| %>
              <li class="col-4-12 ae-3">
                <% if current_user %>
                  <%= link_to table_reservations_path(table), method: :post, class: "cell-73 equalElement" do %>
                      <h3 class="uppercase"><%="#{table.table_name}"%></h3>
                      <p class="small"><%="$#{table.minimum}"%></p>
                      <% end %>
                    <% else %>
                      <%= link_to "Table #{table.table_name} $#{table.minimum}", table_path(table) %>
                    <% end %>
                    <%= link_to "Delete Table", club_table_path(@club, table), method: :delete if @current_club %>
                    <%= link_to "Edit Minimum", edit_club_table_path(@club, table) if @current_club %>
                  <%end%>
              </li>
            <% end %>
            <%= link_to 'Add Table', new_club_table_path(@club) if @current_club %></div>
          </ul>


  <br>
    <ul>
      <% if not @club.bottles.nil? %>
        <% @club.bottles.each do |bottle| %>
            <li><% if current_club %>
              <h2>Bottle Menu</h2>
                  <%= "Bottle #{bottle.name} $#{bottle.price}" %>
                  <%= link_to 'Delete Bottle', club_bottle_path(@club, bottle), method: :delete if @current_club %>
                  <%= link_to 'Edit Bottle', edit_club_bottle_path(@club, bottle) if @current_club%> <br>
                <% end %>
            </l1>
        <% end %>
      <% end %>
          </ul>
          <%= link_to 'Add Bottle', new_club_bottle_path(@club) if @current_club %>
        </div>
      </div>
    </div>
  </div>
  
  <div class="background" style="background-image:url(<%= image_url('img-clubs.jpeg') %>)"></div>
</section>

<!-- Preloader -->
<div class="loadingIcon">
  <svg class="loading-icon" id="loading-circle" viewBox="0 0 18 18">
    <circle class="circle" opacity=".1" stroke="#fff" stroke-width="2" stroke-miterlimit="10" cx="9" cy="9" r="8" fill="none"></circle>
    <circle class="dash" stroke-width="2" stroke-linecap="round" stroke-miterlimit="10" stroke-dasharray="1,100" cx="9" cy="9" r="8" fill="none"></circle>
  </svg>
</div>

